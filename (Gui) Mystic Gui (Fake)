-- Scripts:

local function LRSGZDF_fake_script() -- Run_2.LocalScript 
	local script = Instance.new('LocalScript', Run_2)

	player = game.Players.LocalPlayer
	Open = false
	local Mouse = game.Players.LocalPlayer:GetMouse()
	function onKeyDown(key)
		if string.byte(key) == 48 then
		if Open == false then
			Open = true
			script.Parent.Text = "Walk(Shift)"
			player.Character.Humanoid.WalkSpeed = 16 + 35
		elseif Open == true then
			Open = false
			script.Parent.Text = "Run(Shift)"
			player.Character.Humanoid.WalkSpeed = 16
		end
		end
	end
	Mouse.KeyDown:connect(onKeyDown)
	function MouseClick()
		if Open == false then
			Open = true
			script.Parent.Text = "Walk(Shift)"
			player.Character.Humanoid.WalkSpeed = 16 + 35
		elseif Open == true then
			Open = false
			script.Parent.Text = "Run(Shift)"
			player.Character.Humanoid.WalkSpeed = 16
		end
		
	end
	script.Parent.MouseButton1Down:connect(MouseClick)
end
coroutine.wrap(LRSGZDF_fake_script)()
